<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - webpages</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/webpages.styles.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/js/site.js" asp-append-version="true" />
  
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0-beta3/css/all.min.css" rel="stylesheet">
    

</head>
<body>
    <header>
        <nav class="navbar navbar-expand-lg navbar-light bg-black text-white bg-body-tertiary">
            <div class="container-fluid">
                <a class="navbar-brand text-white"  href="#">
                    <img src="~/pencil pic.jpg" alt="Alternate Text" width="50px"/>
                </a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <ul class="navbar-nav me-auto  text-white mb-2 mb-lg-0">
                        <li class="nav-item ">
                        <a class="nav-link active  text-white" aria-current="page" asp-controller="Web" asp-action="Home">Home</a>
                        </li>
                        <li class="nav-item">
                        <a class="nav-link text-white" aria-current="page" asp-controller="Web" asp-action="About">About</a>
                        </li>
                        <li class="nav-item">
                        <a class="nav-link text-white" aria-current="page" asp-controller="Web" asp-action="Gallery">Gallery</a>
                        </li>
                        <li class="nav-item">
                        <a class="nav-link text-white" aria-current="page" asp-controller="Web" asp-action="Services">Services</a>
                        </li>
                        <li class="nav-item">
                        <a class="nav-link text-white" aria-current="page" asp-controller="Web" asp-action="Contact">Contact</a>
                        </li>
                        
                       
                       
                    </ul>
                    <form class="d-flex" role="search">
                        <input class="form-control me-2" type="search" placeholder="Search" aria-label="Search">
                        <button class="btn btn-outline-success" type="submit">Search</button>
                    </form>
                </div>
            
        </nav>
      
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2024 - webpages - <a asp-area="" asp-controller="Web" asp-action="Home">Privacynnnn</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
